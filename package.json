{
    "name": "tt-package-demo",
    "version": "1.0.0",
    "description": "A demo package for Total TypeScript",
    "keywords": [
        "demo",
        "typescript"
    ],
    "scripts": {
        "build": "tsc",
        "ci": "npm run build && npm run check-format && npm run test",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "check-exports": "attw --pack .",
        "test": "vitest run",
        "dev": "vitest"
    },
    "homepage": "https://github.com/mattpocock/tt-package-demo",
    "main": "dist/index.js",
    "bugs": {
        "url": "https://github.com/mattpocock/tt-package-demo/issues"
    },
    "author": "Filip Kovac",
    "repository": {
        "type": "git",
        "url": "https://github.com/KovacFilip/tt-package-demo"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "license": "MIT",
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.15.4",
        "prettier": "^3.3.3",
        "typescript": "^5.5.4",
        "vitest": "^2.0.5"
    }
}